@using static THNETII.CdnJs.PopperJS.PopperJSConstants

<environment include="Development">
    <script src="~/@Source.LocalUrl"></script>
</environment>
<environment exclude="Development">
    <script src="@Minified.Url"
            asp-fallback-src="~/@Minified.LocalUrl"
            asp-fallback-test="@AspFallbackTest"
            integrity="@Minified.Sri" crossorigin="anonymous"></script>
</environment>
