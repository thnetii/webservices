@using static THNETII.CdnJs.JQuery.JQueryConstants

<environment include="Development">
    <script src="~/@Slim.LocalUrl"></script>
</environment>
<environment exclude="Development">
    <script src="@SlimMinified.Url"
            asp-fallback-src="~/@SlimMinified.LocalUrl"
            asp-fallback-test="@AspFallbackTest"
            integrity="@SlimMinified.Sri" crossorigin="anonymous"></script>
</environment>
