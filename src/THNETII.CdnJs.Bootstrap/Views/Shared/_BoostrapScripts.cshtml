@using static THNETII.CdnJs.Bootstrap.BootstrapConstants

<environment include="Development">
    <script src="~/@JavaScript.LocalUrl"></script>
</environment>
<environment exclude="Development">
    <script src="@JavaScriptMinified.Url"
            asp-fallback-src="~/@JavaScriptMinified.LocalUrl"
            asp-fallback-test="@JavaScript.AspFallbackTest"
            integrity="@JavaScriptMinified.Sri" crossorigin="anonymous"></script>
</environment>
